{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins":[
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "semi": 0,
    "max-len": [2, 256, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "react/jsx-one-expression-per-line": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}